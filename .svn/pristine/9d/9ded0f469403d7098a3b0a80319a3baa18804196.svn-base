@import url('https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap');
*{
	font-family: 'Lato','SpoqaHanSansNeo', sans-serif;
	font-size:14px;
	color:#666;
	letter-spacing:-0.5px;
	line-height:1;
    box-sizing: border-box;
    outline: none;
	margin: 0;
	padding: 0;
}
img{
	border:0;
	max-width:100%;
	vertical-align: middle;
}
a, a:hover, a:focus{
	text-decoration:none;
	-webkit-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}
span, em, strong, b{
	font-size:100%;
	color:inherit;
	font-family:inherit;
}
button{
    outline: none;
    border: 0;
    background: none;
}
ul, ol, li {
	list-style:none;
}
/*Removes default chrome and safari style*/
select{ 
	-webkit-appearance: none; 
	-moz-appearance: none;
	appearance: none;
}
select::-ms-expand {
   display: none;          
}
table{
	border-collapse:collapse;
}
input[type="text"]{border:0; }
.dis-no{display:none !important;}
.vis-no{visibility:hidden !important;}
.table{
	display:Table;
	width:100%;
	table-layout:fixed;	
}
.table > div{
	display:Table-cell;
}





@font-face {
    font-family: 'SpoqaHanSansNeo';
    src: url(/font/SpoqaHanSansNeo-Regular.eot);
  	src: local('SpoqaHanSansNeo-Regular'),
       local('SpoqaHanSansNeo-Regular'),
       url(/font/SpoqaHanSansNeo-Regular.eot?#iefix) format('embedded-opentype'),
