.navy{color: #262f40;}
.white{color: #fff;}
.black{color: #000;}

.container01{width: 100%; height: 100%; display: flex; align-items: center; justify-content: space-between;}
.container02{width: calc(99.999% - 10.417vw); height: 100%; position: absolute; top: 0; left: 0;} 
/* header */
.header{width: 100%; height: 112px; position: fixed; top: 0; left: 0; z-index: 1000;}
.header-left{width: calc(99.999% - 10.417vw); height:100%; display: flex; padding: 0 3.646vw;  align-items: center;}
.header-right{width: 11.458vw; display: flex;}
.header h1.logo{}
.header h1.logo img{display: block; align-items: center;}
.header h1.logo img.mo-ver{display:none;}
.header .maingnb-wrap{width: calc(99.999% - 11.042vw); height: 100%; display: flex; justify-content: center; align-items: center;text-align: center;}
.header .maingnb{display: flex; height: 100%; justify-content: center; align-items: center;  text-align: center;}
.header .maingnb > li{ float: left; margin-right: 3.750vw; height: 100%; display: flex;  align-items: center; position:relative;}
.header .maingnb > li:last-child{margin-right: 0;}
.header .maingnb > li > a{font-size: 18px; color: #000; transition: none;}
.header .maingnb > li > a:hover{color:#9fbee4 !important;}
.dep2{width:225px; height:200px; position:absolute; top:100%; left:50%; transform:translateX(-50%); padding-top:35px; background:#fff; display:none;}
.dep2 li{width:100%;}
.dep2 li a{display:block; width:100%; font-size:16px; color:rgba(0,0,0,0.5); text-align:center; padding-bottom:25px;}
.dep2 li.on a{color:#262d3f;}

.se-lang{display: flex; align-items: center; display: none;}
.se-lang > img{margin-right: 50px;;}
.lang{display: flex; align-items: center;}
.lang > img:first-child{margin-right: 8px;}
.lang > img:last-child{margin-left: 16px;}
.lang li:last-child{display: none;}
.lang li a{font-size: 15px; font-weight: bold;}
.hammenu{ width: 10.417vw; display: flex; justify-content: center;}
.hammenu span{display: block;}
.hammenu img{display:block; cursor:pointer;}
.hammenu.pc-ver img:nth-child(2){display:none;}
.hammenu.pc-ver.on img:nth-child(1){display:none;}
.hammenu.pc-ver.on img:nth-child(2){display:block;}
.hammenu img.mo-ver{display:none;}
#fp-nav{}
#fp-nav.fp-right{transition: background 1s; margin-top: 0; width: 10.417vw;height: 100%; top:0; right: 0;  display: flex !important;justify-content: center;align-items: center; border-left: 1px solid rgba(0,0,0,0.1);}
#fp-nav.fp-right.last{z-index:-1;}
#fp-nav ul{display: flex; flex-direction: column;}
#fp-nav ul li{width: 6px; height: 6px; margin-bottom: 3.906vw;}
#fp-nav ul li:last-child{display: none;}
#fp-nav ul li:nth-child(5){margin-bottom: 0;}
#fp-nav ul li a span{width: 6px; height: 6px; color: #262f40; opacity: 0.3; border-radius: 0;}
#fp-nav ul li a.active span{opacity: 1; border-radius: 0;}
#fp-nav .fp-sr-only{display: none;}
#fp-nav ul li a.active span,
#fp-nav ul li:hover a.active span, 
.fp-slidesNav ul li a.active span, 
.fp-slidesNav ul li:hover a.active span{width:6px; height:6px; margin: -3px 0 0 -3px;}


.down-scroll{position: fixed; bottom: 10%; right: 4.5%; width: 30px; height: 30px; border-radius: 100%; border: 1px solid rgba(255,255,255,0.5);  z-index: 1000; cursor:pointer;}
.down-scroll.mo-ver{display:none !important;}
.down-scroll img{position: absolute; top: 50%; left: 50%; transform: translate(-50%,-50%); transition:all 0.5s;}
.down-scroll::after{content: ''; display: block; width: 1px; height: 60px; position: absolute; bottom: 20px; left: 50%; transform: translateX(-50%); background: #262f40; transition:all 0.5s;}
.down-scroll.last::after{bottom:-199%;}
.down-scroll.last img{transform: translate(-50%,-50%) rotate(180deg);}
.down-scroll.white::after{background: #fff;}

.pc-ver{}
.mo-ver{display:none;}


/*2021.12.13 추가*/
.header.fixed {background: #fff; transition: all ease .5s}
.header {transition: all ease .5s}

